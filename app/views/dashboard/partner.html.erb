    <% chart.add_rows(
        @partner.company_counters.map do |counter|
          [counter.happened_at.to_s_date, counter.click]
        end
       ) %>
  <% end %>

  <div class="content-separator"></div>
  <h3>应聘数</h3>
  <% visualization "job_app_count_chart", "LineChart", :width => 635, :height => 200, :html => {:class => "graph_chart"} do |chart| %>
    <% chart.string "日期" %>
    <% chart.number "查看数" %>
    <% chart.add_rows(
        @partner.job_application_counters.map do |counter|
          [counter.happened_at.to_s_date, counter.click]
        end
       ) %>
  <% end %>

  <div class="content-separator"></div>
  <h3>注册应聘者</h3>
  <% visualization "jobseeker_count_chart", "LineChart", :width => 635, :height => 200, :html => {:class => "graph_chart"} do |chart| %>
    <% chart.string "日期" %>
    <% chart.number "查看数" %>
    <% chart.add_rows(
        @partner.jobseeker_counters.map do |counter|
          [counter.happened_at.to_s_date, counter.click]
        end
       ) %>
  <% end %>

</fieldset>