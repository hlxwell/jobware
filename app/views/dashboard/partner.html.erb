<fieldset>
  <legend>报表</legend>
  <h3>发布岗位数</h3>
  <% visualization "job_count_chart", "LineChart", :width => 635, :height => 200, :html => {:class => "graph_chart"} do |chart| %>
    <% chart.string "日期" %>
    <% chart.number "查看数" %>
    <% chart.add_rows(
        @partner.job_counters.map do |counter|
          [counter.happened_at.to_s_date, counter.click]
        end
       ) %>
  <% end %>

  <div class="content-separator"></div>
  <h3>注册公司数</h3>
  <% visualization "company_count_chart", "LineChart", :width => 635, :height => 200, :html => {:class => "graph_chart"} do |chart| %>
