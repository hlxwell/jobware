<div class="main" id="main-two-columns-right-sidebar">

	<div class="right sidebar" id="sidebar-1">
		Empty sidebar 1.
	</div>

	<div class="left" id="main-left">
	  <h2>注册合作站</h2>

	  <form>
    		<fieldset>
          <legend>网站信息</legend>

          <div class="error form-error">错误信息</div>

          <p>
      			<label for="input-1">网站名</label><br/>
      			<input type="text" name="input-1" id="input-1" class="text w600" />
      		</p>

      		<p>
      			<label for="input-1">网站地址</label><br/>
      			<input type="text" name="input-1" id="input-1" class="text w600" />
      		</p>

      		<p>
      			<label for="input-1">联系人</label><br/>
      			<input type="text" name="input-1" id="input-1" class="text w600" />
      		</p>

      		<p>
      			<label for="input-1">联系电话</label><br/>
      			<input type="text" name="input-1" id="input-1" class="text w600" />
      		</p>

      		<p>
      			<label for="input-1">联系邮箱</label><br/>
      			<input type="text" name="input-1" id="input-1" class="text w600" />
      		</p>

      		<p>
      			<label for="input-2">网站规模</label><br/>
      			<select name="input-2" id="input-2">
      				<option>每天1000-3000PV</option>
      				<option>Option 2</option>
      				<option>Option 3</option>
      			</select>
      		</p>

      		<p>
      			<label for="input-3">网站描述</label><br/>
      			<textarea name="input-3" id="input-3" rows="10" cols="90" class="w600"></textarea>
      		</p>
        </fieldset>

        <fieldset>
          <legend>登陆帐号</legend>

          <div class="error form-error">错误信息</div>

      		<p>
      			<label for="input-1">邮箱</label><br/>
      			<input type="text" name="input-1" id="input-1" class="text w600" />
      			<span class="helper">长度控制在10到20位。</span>
      		</p>

      		<p>
      			<label for="input-1">密码</label><br/>
      			<input type="text" name="input-1" id="input-1" class="text w600" />
      			<span class="helper">长度控制在10到20位。</span>
      		</p>

      		<p>
      			<label for="input-1">确认密码</label><br/>
      			<input type="text" name="input-1" id="input-1" class="text w600" />
      			<span class="helper">长度控制在10到20位。</span>
      		</p>
        </fieldset>

        <p><input type="submit" class="button right" value="提交" /></p>
    </form>
	</div>

	<div class="clearer">&nbsp;</div>
</div>