<%= link_to "审核通过 (approve)", :controller => "admin/partners", :action => "approve", :id => @item.id %>
<%= link_to "拒绝 (reject)", :controller => "admin/partners", :action => "reject", :id => @item.id %>

<%= jobware_form_for :commission, :url => {:controller => "admin/partners", :action => "send_commission", :id => @item.id}, :title => "结算佣金" do |f| %>
  <%= f.text_field :amount, :label => "佣金数量" %>
  <%= f.text_field :money_from, :label => "佣金来源" %>
  <%= f.submit "提交" %>
<% end %>