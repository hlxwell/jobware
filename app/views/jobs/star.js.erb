$("#rating small").html("<%= @result %>");

// used in jobseeker management
$("#job_<%= @job.id %> span").html("<%= @result %>");
$("#job_<%= @job.id %> span").fadeIn("slow");
$("#job_<%= @job.id %> span").fadeOut(2000);

if("<%= @result %>" == "取消成功") {
  $("#job_<%= @job.id %>").fadeOut("slow");
}