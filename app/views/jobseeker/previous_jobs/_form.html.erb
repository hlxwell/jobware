<%= form_for @previous_job do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :resume_id %><br />
    <%= f.text_field :resume_id %>
  </p>
  <p>
    <%= f.label :company_name %><br />
    <%= f.text_field :company_name %>
  </p>
  <p>
    <%= f.label :company_type %><br />
    <%= f.text_field :company_type %>
  </p>
  <p>
    <%= f.label :company_size %><br />
    <%= f.text_field :company_size %>
  </p>
  <p>
    <%= f.label :job_title %><br />
    <%= f.text_field :job_title %>
  </p>
  <p>
    <%= f.label :start_at %><br />
    <%= f.datetime_select :start_at %>
  </p>
  <p>
    <%= f.label :end_at %><br />
    <%= f.datetime_select :end_at %>
  </p>
  <p><%= f.submit %></p>
<% end %>
