<%= form_for @school do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :resume_id %><br />
    <%= f.text_field :resume_id %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :major %><br />
    <%= f.text_field :major %>
  </p>
  <p>
    <%= f.label :start_at %><br />
    <%= f.datetime_select :start_at %>
  </p>
  <p>
    <%= f.label :end_at %><br />
    <%= f.datetime_select :end_at %>
  </p>
  <p><%= f.submit %></p>
<% end %>
